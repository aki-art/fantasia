{
	"multipart": [
        {
            "apply": {
              "model": "fantasia:block/aspen_log_post"
            }
        },
		{
			"when": {
				"up": "true"
			},
			"apply": {
				"model": "fantasia:block/aspen_log_branch",
				"uvlock": true
			}
		},
		{
			"when": {
				"down": "true"
			},
			"apply": {
				"model": "fantasia:block/aspen_log_branch",
				"x": 180,
				"uvlock": true
			}
		},
		{
			"when": {
				"north": "true"
			},
			"apply": {
				"model": "fantasia:block/aspen_log_branch",
				"uvlock": true,
				"x": 90
			}
		},
		{
			"when": {
				"east": "true"
			},
			"apply": {
				"model": "fantasia:block/aspen_log_branch",
				"y": 90,
				"uvlock": true,
				"x": 90
			}
		},
		{
			"when": {
				"south": "true"
			},
			"apply": {
				"model": "fantasia:block/aspen_log_branch",
				"y": 180,
				"uvlock": true,
				"x": 90
			}
		},
		{
			"when": {
				"west": "true"
			},
			"apply": {
				"model": "fantasia:block/aspen_log_branch",
				"y": 270,
				"uvlock": true,
				"x": 90
			}
		}
	]
}
