      },
      "apply": {
        "model": "fantasia:block/aspen_stripped_log_branch",
        "x": 90,
        "uvlock": true
      }
    },
    {
      "when": {
        "east": "true"
      },
      "apply": {
        "model": "fantasia:block/aspen_stripped_log_branch",
        "x": 90,
        "y": 90,
        "uvlock": true
      }
    },
    {
      "when": {
        "south": "true"
      },
      "apply": {
        "model": "fantasia:block/aspen_stripped_log_branch",
        "x": 90,
        "y": 180,
        "uvlock": true
      }
    },
    {
      "when": {
        "west": "true"
      },
      "apply": {
        "model": "fantasia:block/aspen_stripped_log_branch",
        "x": 90,
        "y": 270,
        "uvlock": true
      }
